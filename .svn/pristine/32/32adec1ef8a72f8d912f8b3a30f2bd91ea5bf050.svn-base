<script type="text/javascript">
    //银行卡体现
    $(function () {
    	var NowDate = new Date();
        var H = NowDate.getHours();
        var M = NowDate.getMinutes();

        //当前时间
        var nowtime = H*60+M;
        //早上时间
        var morning = 9*60;
        //下午时间
        var noon = 15*60;

        if ((nowtime<=morning) || (nowtime>=noon)){
            msg("该时间段内不可提现");
            window.setTimeout(function(){ location.href="<?php echo U('User/memberinfo');?>";},2000);
            }

    	if ($('#utel').val()=='') {
        msg("请先完善联系电话");
        //window.setTimeout(function(){window.location.href="/index.php/Home/User/editle.html";},1000);
        window.setTimeout(function(){ location.href="<?php echo U('User/editle');?>";},1000);
       }
    	$('.banks').click(function(){
    		if($(this).attr('class').indexOf('hover')>0){
    			$(this).removeClass('hover');
    		}else{
    			$(this).addClass('hover');	
    		}
    		
    	})
    	if($('#pwd').val()==''){
    		msg("请先完善提现密码");
    		window.setTimeout(function(){ location.href="<?php echo U('User/pwdadd');?>";},1000);
    	}
    	
	    $("#n-money").blur(function(){
	       	var jine=$(this).val();
	        if(jine<100){
	       		msg('亲，提现金额不能少于100元！');
	        }
		});
	     
	     
	    $('.b-close1').click(function(){
	        $('.withdraw-box,.mask').hide();
	    });
	    $('.b-close2').click(function(){
	        $('.tixing-box,.mask').hide();
	    });
	    $('.b-close').click(function(){
	        window.location.href='/money';
	    });
    
    
    
    var $wForm = $('#withDraw'),			//form表单
		$nMoney = $('#n-money'),			//提现金额
        $bankChoose = $('#Bank'),			//银行
        $cardNum = $('#cardNum'),			//卡号
        $wName = $('#Name'),				//姓名
        $idNum = $('#idNum'),				//支行
        $dPwd = $('#deal-pwd'),	 			//交易密码
		$province=$("#selProvince"),		//省份
    	$city=$("#selCity"),  				//城市
    	$counts=$("#counts").val(),			//账户余额
      	errorInfo = '服务器忙，请稍后重试';

		$wForm.submit(function(){
			
      		var postData = {};
      		
      		if($counts<=0){
      			msg('您的账户没有金额不能提现!');
          		return false;
      		}
          	if($nMoney.val() =='' ){
            	msg('输入金额格式不正确！');
            	$nMoney.focus();
            	return false;
       		}else if($nMoney.val()<100){
       			msg('您提现金额不能低于100！');
            	$nMoney.focus();
            	return false;
       		}
         	else if((parseInt($nMoney.val()))>parseInt($counts)+1){
      			msg('提现金额不能大于账户余额！');
                $nMoney.focus();
                return false;
      		}
      		if($bankChoose.val() == '0') {
                msg('请选择银行！');
                $bankChoose.focus();
                return false;
            }
            if($province.val()==''){
         		msg('请选择省份');
                $province.focus();
                return false;
       		}
	        if($city.val()==''){
	          	msg('请选择城市');
                $city.focus();
                return false;
	       	}
       		if($idNum.val()=='') {
                msg('银行支行名称格式不正确！');
                $idNum.focus();
                return false;
            }      
      		if($cardNum.val()=='') {
                msg('卡号格式不正确！');
                $cardNum.focus();
                return false;
            }
            if($wName.val() == '') {
                msg('姓名不能为空！');
                $wName.focus();
                return false;
            }
            if($dPwd.val()=='') {
                msg('请输入提现密码！');
                $dPwd.focus();
                return false;
            }
            postData = $(this).serialize();
            $.ajax({
                type: 'POST',
                url: '<?php echo U("User/cash");?>',
                data: postData,
                dataType: 'json',
				timeout:200000,
				beforeSend:function(){
            		$("#pwds").show();
				},
                success: function (data) {
                	$("#pwds").hide();
		            if(data==1){
		                $("#je").html($nMoney.val());
		              	$(".mask").show();
		                $("#crSuccess").show();
		            }
  					else if(data==-3){                  
       					$(".f-sub").removeAttr("disabled");
        				msg('请输入正确的金额');
   					}
		            else if(data==10){
		               	$(".f-sub").removeAttr("disabled");
		                msg('银行帐号信息错误请重新提现！');			           
		           	}
   					else if(data==0){
         				msg('提现失败!');
   					}else if(data==-88){
              			$(".f-sub").removeAttr("disabled");
           				msg('非法操作');
   					}
        			else if(data==-99){
              			$(".f-sub").removeAttr("disabled");
           				msg('请输入正确的提现密码！');
   					}
   					// else if(data==2){
        //       			$(".f-sub").removeAttr("disabled");
        //    				msg('该时间段不允许提现');
   					// }
  					else{               
       					$(".f-sub").removeAttr("disabled");  
            			msg('操作成功！正在审核中....');
            			//防止重复提交
            			$('.f-sub').attr('disabled',"true");  
						//2秒后跳转代码
						window.setTimeout(function(){ location.href ="<?php echo U('User/memberinfo');?>"; },4000);
  					}
        
        
    			},
    			error: function () {
                  	//$("#pwds").hide();
         			//$(".f-sub").removeAttr("disabled");
        			msg(errorInfo);
                 	window.location.reload();        
    			}
        	});
            return false;
        });
    });

