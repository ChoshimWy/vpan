/*数据*/
.chart{position:static;padding:10px;background:#F6F6F6;}
.chart ul{border:#ddd 1px solid;border-radius:5px;background:#fff;padding:15px;}
.chart li{height:33px;line-height:33px;font-size:1em;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;border-top:#ddd 1px solid;background:url(../images/link.png) right no-repeat;}
.chart li:first-child{border-top:none;}
.chart li a{display:block;vertical-align:top;padding:0 5px;}
/*数据内容*/
.datacontent{position:static;width:100%;}
.data-info{padding:0 15px;position:relative;}
.data-info h1{font-size:1.5em;font-weight:normal;height:40px;line-height:40px;display:block;width:100%;}
.data-info h2{font-size:3em;font-weight:normal;height:58px;line-height:58px;}
.data-info h2 i{font-size:0.4em;font-style:normal;margin-left:5px;}
.data-container{margin-top:20px;}
.data-history{position:static;width:100%;margin:20px 0;}
.data-history li{border-bottom:#ddd 1px solid;height:40px;line-height:40px;}
.data-history li span{float:left;width:29%;text-align:right;padding-right:4%;display:inline-block;vertical-align:top;font-size:0.8em;}
.data-history li span:first-child{width:28%;text-align:left;padding:0 0 0 4%;}
.data-history li.CodeHead{background:#eee;height:32px;line-height:32px;}
.data-history li.CodeHead span{color:#999}
.data-history li .before,.data-history li .affect,.data-history li .etfAs,.data-history li .etfTal{display:none;}
