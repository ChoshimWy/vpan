@media only screen and (max-width:481px){/*iphone 6 plus*/
.news li,.comment li{height:65px;padding:13px;}
.news li img{width:90px;height:65px;}
.news li h4{padding-left:105px;font-size:1em;line-height:20px;max-height:44px;overflow:hidden;}
.news li p{padding-left:105px;margin-top:5px;height:20px;line-height:20px;font-size:0.7em;color:#999;}
.news li p a{height:18px;line-height:18px;padding:0 6px;margin-right:6px;}
.sec-week{height:85px;}
.sec-week li{width:48px;line-height:25px;height:85px;}
.sec-week li i{font-size:2em;line-height:1.6em;}
.sec-week li.cur i{border-bottom:#2577E3 solid 3px;}
.comment li h4{padding-left:105px;font-size:1.2em;line-height:20px;height:22px;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;vertical-align:top;}
.comment li img{width:90px;height:auto;}
.comment li p{padding-left:105px;padding-top:5px;max-height:42px;line-height:20px;font-size:0.8em;color:#999;overflow:hidden;}
.price-info{padding:15px 15px 0px;font-size:1.2em;}
.price-info h1{font-size:1.2em;font-weight:normal;height:40px;line-height:40px;}
.price-info h2{font-size:2.4em;font-weight:normal;height:70px;line-height:70px;}
.price-info p{bottom:0;right:15px;height:70;}
.price-info p span{line-height:35px;height:35px;}
.data-info{margin-top:15px;}
.data-info ul{height:70px;}
.data-info li{height:68px;line-height:45px;}
.data-nav{margin:15px 15px 0;height:40px;line-height:40px;font-size:1em;}
.setbox{width:100%;}
.setbox dl{margin:0 15px;}
.setbox dt{display:none;}
}
@media only screen and (max-width:401px) {/*iphone 6  375*/
.sec-week{height:85px;}
.sec-week li{width:43px;line-height:25px;height:85px;}
.sec-week li i{font-size:2em;line-height:1.6em;}
.sec-week li.cur i{border-bottom:#2577E3 solid 3px;}
.comment li img{width:90px;height:auto;}
.price-info{padding:13px 13px 0px;font-size:1.2em;}
.price-info h1{font-size:1.2em;font-weight:normal;height:30px;line-height:30px;}
.price-info h2{font-size:2.4em;font-weight:normal;height:60px;line-height:60px;}
.price-info p{bottom:-10px;right:13px;height:60;}
.price-info p span{line-height:30px;height:30px;}
.data-info{margin-top:13px;}
.data-info ul{height:64px;}
.data-info li{height:62px;line-height:40px;}
.data-nav{margin:13px 13px 0;height:36px;line-height:36px;font-size:1em;}
.setbox dl{margin:0 10px;}
.setbox a{width:50px;}
}

@media only screen and (max-width:361px) {/* 安卓手机  360*/
.sec-week li{width:40px;line-height:25px;height:85px;}
.price-info{font-size:1em;}
.price-info h1{font-size:1em;height:26px;line-height:26px;}
.price-info h2{font-size:2em;height:50px;line-height:50px;}
.price-info p{bottom:-8px;right:13px;height:50;}
.price-info p span{line-height:25px;height:25px;}
.data-info{margin-top:10px;}
.data-info ul{height:58px;}
.data-info li{height:56px;line-height:35px;}
.setbox dl{margin:0 10px;}
.setbox dd{padding-left:24px;}
.setbox a{width:46px;}
}
@media only screen and (max-width:321px) {/*iphone 5s 及以下*/
.sec-week{height:75px;}
.sec-week li{width:35px;}
.sec-week li i{font-size:1.6em;}
.kuai li span{margin-right:5px;}
.kuai li .duokongBox{margin-right:0;}
.setbox dl{margin:0;}

